report_b <- function(coefficient = NULL, markdown = T, standardize = F){
if(markdown == T){
bval <- sub("^(-?)0.", "\\1.", sprintf("%.3f", coefficient))
if(standardize == F){
b_report <- paste("*b* =", bval)
}
if(standardize == F){
b_report <- paste("\beta", " = ", bval, sep = "")
}
}
if(markdown == F){
bval <- sub("^(-?)0.", "\\1.", sprintf("%.3f", coefficient))
if(standardize == F){
b_report <- paste("b =", bval)
}
if(standardize == F){
b_report <- paste("beta", " = ", bval, sep = "")
}
}
return(noquote(b_report))
}
