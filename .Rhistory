report_p <- function(pvalue = NULL, markdown = T){
if(markdown = T){
if(round(x, 3) > 0){ p_report <- paste("*p* =", round(x, 3)) }
else(round(x, 3) == 0){ p_report <- paste("*p* < .001") }
} # end markdown = T if statement
if(markdown = F){
if(round(x, 3) > 0){ p_report <- paste("p =", round(x, 3)) }
else(round(x, 3) == 0){ p_report <- paste("p < .001") }
} # end markdown = F if statement
return(noquote(p_report))
}
